{
  "name": "laravel/laravel",
  "type": "project",
  "description": "The Laravel Framework.",
  "keywords": [
    "framework",
    "laravel"
  ],
  "license": "MIT",
  "require": {
    "php": "^7.4||^8.0",
    "ext-json": "*",
    "ext-mbstring": "*",
    "doctrine/dbal": "^2.9",
    "fideloper/proxy": "^4.2",
    "fruitcake/laravel-cors": "^2.0",
    "guzzlehttp/guzzle": "^7.0.1",
    "intervention/image": "^2.4",
    "laravel-notification-channels/discord": "^1.0",
    "laravel/framework": "^8.0",
    "laravel/horizon": "^5.0",
    "laravel/slack-notification-channel": "^2.0",
    "laravel/socialite": "^5.0",
    "laravel/tinker": "^2.0",
    "league/commonmark": "^1.4",
    "predis/predis": "^1.1",
    "revolution/laravel-namespaced-helpers": "^1.0",
    "revolution/laravel-pagination-bulma": "^1.0",
    "revolution/laravel-server-push": "^1.0",
    "revolution/laravel-str-mixins": "^2.0",
    "spatie/laravel-feed": "^3.0"
  },
  "require-dev": {
    "barryvdh/laravel-ide-helper": "^2.6",
    "facade/ignition": "^2.0",
    "fakerphp/faker": "^1.9.1",
    "mockery/mockery": "^1.3",
    "nunomaduro/collision": "^5.0",
    "phpunit/phpunit": "^9.0"
  },
  "conflict": {
    "cakephp/core": "*"
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  },
  "extra": {
    "laravel": {
      "dont-discover": []
    }
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/",
      "Database\\Factories\\": "database/factories/",
      "Database\\Seeders\\": "database/seeders/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "scripts": {
    "post-autoload-dump": [
      "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
      "@php artisan package:discover --ansi"
    ],
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": [
      "@php artisan key:generate --ansi"
    ],
    "ide-helper:meta": "php artisan ide-helper:meta",
    "ide-helper:generate": "php artisan ide-helper:generate",
    "ide-helper:models": "php artisan ide-helper:models -N"
  }
}
